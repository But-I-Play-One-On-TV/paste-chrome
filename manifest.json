{
  "manifest_version": 2,

  "name": "Paste by Write.as",
  "description": "Effortlessly share text online.",
  "version": "1.0",

  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Publish this"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*.write.as/",
    "https://fonts.googleapis.com/"
  ]
}
